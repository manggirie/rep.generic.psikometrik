@model  IEnumerable<Bespoke.epsikologi_skorppkp.Domain.SkorPpkp>


<table class="ppkp-dimensi">
    <colgroup>
        <col style="width: 180px" />
        <col style="width: 20px" />
        <col style="width: 600px" />
    </colgroup>
    <tbody>
        @foreach (var score in Model)
        {
            <tr>
                <td style="vertical-align: top"><b>@score.NamaTret ; @score.Tahap - </b>@score.Interpretasi</td>
            </tr>
            <tr>
                <td colspan="3"></td>
            </tr>
        }

    </tbody>
</table>

