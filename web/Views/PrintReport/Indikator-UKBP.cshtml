    <tr>
        <td>1</td>
        <td>Kebolehpercayaan</td>
        
        @for (var i = 1; i <= 15; i++)
        {
            var td = Model.TdCollection.SingleOrDefault(x => x.Tret == UkbpTraitViewModel.KEBOLEHPERCAYAAN && x.Value == i);
            if (null == td)
            {
                throw new Exception($"Cannot find {i}");
            }
            <td>
                <img alt="@i" src="/images/ukbp/@(td.Image).png" id="kby-@i"/>
            </td>
        }
        <td colspan="5"></td>
    </tr>
