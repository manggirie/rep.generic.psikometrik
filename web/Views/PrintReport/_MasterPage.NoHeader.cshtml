<html>
<head>
    <title>Laporan untuk @Model.Sesi.NamaPengguna</title>
    <link rel="stylesheet" type="text/css" href="/content/external/bootstrap.min.css">
    <style media="print">
        div.peringatan {
            page-break-after: always;
        }
    </style>
    <style>
        div.peringatan {
            page-break-after: always;
        }
    </style>
    @RenderSection("head", false)
</head>
<body>
    @if (IsSectionDefined("Peringatan"))
    {
        <div>
            @RenderSection("Peringatan")
        </div>
    }
    else
    {
        <div>

            @Html.Partial("_peringatan.ip")
        </div>
    }
    @RenderBody()
</body>
</html>
